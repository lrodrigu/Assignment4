<ht	ml>
	<head>
		
		<title>NYC Soccer Foundation</title>
		<link rel="Stylesheet" href="css/normalize.css"/>
		<link rel="Stylesheet" href="css/bootstrap.css"/>
		<link rel="Stylesheet" href="css/myCustom.css"/>
	
	<style>

/*	*{
		border:solid 1px black;
	}*/



	</style>

	</head>
	
	<body>

		<div class="row" id="firstbar">
			
			<div class="col-md-9">
				<a style="color:inherit; text-decoration:none;" href="index"><span class="nyc">NEW YORK CITY</span> SOCCER FOUNDATION</a>
			</div>

			<div class="col-md-2">
				
				<a style="color:inherit; text-decoration:none;" href="donate"><div id="donate"> DONATE </div></a>

			</div>
					
		</div>

		<div class="row" id="line">
		</div>

		<div class="container">

			<div class="row" style="height:10px;"></div>

			<div class="row">
				
				<a style="color:inherit; text-decoration:none;" href="whoweare"><div class="col-md-2"  id="navbuttons">Who We Are</div></a>
				<a style="color:inherit; text-decoration:none;" href="programs"><div class="col-md-2"  id="navbuttons"> Programs </div></a>
				<a style="color:inherit; text-decoration:none;" href="media"><div class="col-md-2" id="navbuttons"> Media </div></a>

			</div>

			<div class="row" style="height:30px;"></div>

			<div class="row">	

				<%= yield %>

				<div class="col-md-4">

					<div id="contact">
					
						<form class="form-inline">
						

						<div id="lefta">

								<div class="form-group">
							    	<label for="exampleInputName1">First Name</label>
							    	<input type="text" class="form-control" id="exampleInputName1" placeholder="Jane">
							  	</div>

								<br></br>

								<div class="form-group">
							    	<label for="exampleInputName2">Last Name</label>
							    	<input type="text" class="form-control" id="exampleInputName2" placeholder="Doe">
							  	</div>

								<br></br>				

								<div class="form-group">
							 	    <label for="exampleInputEmail2">Email</label>
							 	    <input type="email" class="form-control" id="exampleInputEmail2" placeholder="jane.doe@example.com">
							 	</div>

								<br></br>				

								<form method="POST" action="/contact">
									<textarea name="comments">Comments...</textarea>
								</form>

							</div>
	
						<button type="submit" class="btn btn-primary">JOIN US</button>
						
						</form>

					</div>

				</div>	

			</div>	

		</div>

		<div class="row" style="height:70px;">
		</div>

		<div class="row" id="line">
		</div>

		<div class="row" id="lastbar">
			
			<a href="https://www.facebook.com" target="blank"><img src="images/social_01.png" width="3%"></a>
			<a href="https://www.linkedin.com" target="blank"><img src="images/social_02.png" width="3%"></a>
			<a href="https://www.pinterest.com" target="blank"><img src="images/social_03.png" width="3%"></a>
			<a href="https://instagram.com/" target="blank"><img src="images/social_04.png" width="3%"></a>
					
		</div>		

		<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
		<script src="js/bootstrap.js"></script>
		<script src="js/custom.js"></script>
		
	</body>
</html>